package com.socialmediapage.digipage.user;

import java.sql.Date;

import javax.persistence.Column;
import javax.persistence.Entity;
import javax.persistence.Table;

@Entity
@Table(name="user")
public class User {

	int id;
    @Column
	String firstname;
    @Column
	String lastname;
	@Column
    String email;
	@Column
	int mobile;
	@Column
	Date dateofbirth;
	@Column
	String gender;
	@Column
	String password;

	
	
}
