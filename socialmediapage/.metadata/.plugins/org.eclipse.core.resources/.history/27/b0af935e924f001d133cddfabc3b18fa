package com.socialmediapage.digipage.user;

import java.sql.Date;

import javax.persistence.Entity;
import javax.persistence.Table;

@Entity
@Table(name="user")
public class User {

	int id;
	String firstname;
	String lastname;
	String email;
	int mobile;
	Date dateofbirth;
	String gender;
	String password;

	
	
}
